{
    "chapterId": "redistribution-and-reorganization",
    "title": "Redistribution & Reorganization",
    "pages": [
        {
            "pageId": 31,
            "transition": "fade",
            "layout": "two-column",
            "media": {
                "mediaId": "main-movie",
